html(lang="en")
  head
    <link rel="stylesheet" href="../Style.css" />
    <meta charset="UTF-8" />
    <script defer type="module" src="../result_page.js"></script>
    title Results

  body(class="web_img")
    nav
      ul(class="top_nav")
        li
          a(href="/index.html") Home
        li
          a(href="/about_page.html") About
        li
          a(href="/contact_page.html") Contact

    h1(class="header") Results

    div(id="eventsTable" class="grid-container")
      div(class="item1") Association
      div(class="item2") Description
      div(class="item3") Link

      each event in events
        div(className="association")= event["association"]
        div(className="description")
          =event["date"] 
          br
          =event["hours"]
          br
          =event["type"]
          br
          =event["description"]
        div(className="link") 
          a(href=event["link"]) LINK
    
